		"fork-ts-checker-webpack-plugin": "^7.2.13",
		"ts-loader": "^9.3.1",
		"webpack-cli": "^4.10.0",
		"webpack-gen": "file:../../webpack-gen"
	},
	"dependencies": {
		"auriserve": "file:../../api/build",
		"common": "file:../../common/build",
