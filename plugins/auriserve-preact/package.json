	"types": "types/Server.d.ts",
	"author": "Auri <me@auri.xyz>",
	"license": "MIT",
	"scripts": {
		"dev": "tsc --watch --project ./tsconfig.json --declaration --emitDeclarationOnly --outDir types > /dev/null & webpack --mode=development --watch --progress --config ./webpack.ts; fg",
