{
	"name": "plugin-dashboard",
	"version": "0.0.1",
	"description": "A Web-based dashboard for plugins.",
	"author": "Auri <me@auri.xyz>",
	"homepage": "https://github.com/AuriServe",
	"license": "AGPL-3.0-only",
	"types": "types/Main.d.ts",
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/AuriServe/AuriServe.git"
	},
	"scripts": {
		"types": "tsc --project ./tsconfig.types.json --outDir types",
		"dev": "tsc --watch --project ./tsconfig.types.json --outDir types > /dev/null & webpack --mode=development --watch --progress --config ./webpack.ts",
		"build": "tsc --project ./tsconfig.types.json > /dev/null & webpack --mode=production --progress --config ./webpack.ts",
		"analyze": "webpack --mode=production --analyze --progress --config webpack.ts"
	},
	"bugs": {
		"url": "https://github.com/AuriServe/AuriServe/issues"
	},
	"devDependencies": {
		"@babel/core": "^7.18.10",
		"@babel/plugin-proposal-class-properties": "^7.18.6",
		"@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
		"@babel/plugin-transform-react-jsx": "^7.18.10",
		"@babel/preset-env": "^7.18.10",
		"@babel/preset-typescript": "^7.18.6",
		"@types/js-cookie": "^3.0.2",
		"@types/routes": "file:../routes",
		"babel-loader": "^8.2.5",
		"css-minimizer-webpack-plugin": "^4.0.0",
		"file-loader": "^6.2.0",
		"fork-ts-checker-webpack-plugin": "^7.2.13",
		"ts-loader": "^9.3.1",
		"webpack-bundle-analyzer": "^4.5.0",
		"webpack-cli": "^4.10.0",
		"webpack-gen": "file:../../webpack-gen",
		"webpack-livereload-plugin": "^3.0.2",
		"webpack": "^5.74.0"
	},
	"dependencies": {
		"@headlessui/react": "^1.6.6",
		"@twind/preset-tailwind": "^1.0.0-next.35",
		"@types/users": "file:../users",
		"auriserve": "file:../../api/build",
		"common": "file:../../common/build",
		"graphql": "^16.5.0",
		"js-cookie": "^3.0.1",
		"preact": "^10.10.2",
		"react-flip-move": "^3.0.4",
		"react-router-dom": "^6.3.0",
		"twind": "^1.0.0-next.35",
		"vibin-hooks": "^0.3.0"
	}
}
